<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="rb">

<!-- 하나의 객실에 대한 예약 내역(결제 완료 상태) 조회.    ROOM_BOOK 테이블에서 Row 여러개 조회 후 반환 -->
<!-- 이미 예약되어서 더이상 예약불가능한 날짜들을 도출하기 위한 과정이므로, BOOK_START_DATE와 BOOK_END_DATE만 받음 -->
	<select id="selectAllBook" parameterType="_int" resultMap="rb">
		SELECT
		BOOK_START_DATE as bookStartDate,
		BOOK_END_DATE as bookEndDate
		FROM ROOM_BOOK WHERE ROOM_NO = ${_parameter}
	</select>

</mapper>
